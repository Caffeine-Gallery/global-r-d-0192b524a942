type Result = 
 variant {
   err: text;
   ok: Article;
 };
type Article = 
 record {
   author: text;
   content: text;
   id: nat;
   timestamp: int;
   title: text;
 };
service : {
  addArticle: (text, text, text) -> (nat);
  getArticle: (nat) -> (Result) query;
  listArticles: () -> (vec Article) query;
}
